<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Show</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <h3>All Users</h3>

    <table>
      <tr>
        <th>Username</th>
        <th>Id</th>
        <th>Email</th>
      </tr>
      <% for(let user of users) { %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
      </tr>
      <% } %>
    </table>
  </body>
</html>
